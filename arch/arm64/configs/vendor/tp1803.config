# Platform
CONFIG_BOARD_TP1803=y

CONFIG_BUILD_ARM64_DT_OVERLAY=y

# HiMax Touchscreen used in TP1803
CONFIG_TOUCHSCREEN_HIMAX_CHIPSET=y
CONFIG_TOUCHSCREEN_HIMAX_I2C=y
CONFIG_TOUCHSCREEN_HIMAX_INCELL=y
CONFIG_TOUCHSCREEN_HIMAX_IC_HX83112=y
# CONFIG_TOUCHSCREEN_HIMAX_DEBUG is not set

# Sensors
CONFIG_SUNWAVE_FINGERPRINT=y
CONFIG_SENSORS_TMD2725=y

# CONFIG_MSM_QBT1000 is not set
# CONFIG_TOUCHSCREEN_SYNAPTICS_TCM is not set
# CONFIG_NFC_NQ is not set
