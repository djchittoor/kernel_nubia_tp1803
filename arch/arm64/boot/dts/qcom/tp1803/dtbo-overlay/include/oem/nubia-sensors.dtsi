&qupv3_se9_i2c {
	status = "ok";
	ams@39 {
		compatible = "tmd2725";
		reg = <0x39>;
		interrupt-parent = <&tlmm>;
		interrupts = <117 0>;
		vdd-supply = <&pm8150l_l7>; //3.0 LEDA
		vio-supply = <&pm8150l_l8>; //1.8 IIC_SDA_PULL IIC_SCL_PULL VDD
		tmd,irq-gpio = <&tlmm 117 0x00>;
		pinctrl-names = "tmd2725_default","tmd2725_sleep";
		pinctrl-0 = <&tmd2725_default>;
		pinctrl-1 = <&tmd2725_sleep>;
		tmd,power_always_on = <1>;
		tmd,has_als = <1>;
		tmd,has_ps = <1>;
		tmd,tp_moudle_count = <3>;
		//[module_id] [which_seg] [d_factor] ch0[1] ch1[0] ch0[1] ch1[0]
		tmd,tp0 = "0","0","652","1000","142","874","99";
		//[module_id] [which_seg] [d_factor] ch0[1] ch1[0] ch0[1] ch1[0]
		tmd,tp1 = "1","1","652","1000","142","874","99";
		//[module_id] [which_seg] [d_factor] ch0[1] ch1[0] ch0[1] ch1[0]
		tmd,tp2 = "2","0","652","1000","142","874","99";
	};
};

&pm8150l_l8 {
	status = "ok";
	regulator-boot-on;
};

&pm8150l_l7 {
	status = "ok";
	qcom,init-voltage = <3100000>;
	regulator-boot-on;
};

&tlmm {
	tmd2725_int_pin {
		tmd2725_default: tmd2725_default {
			config {
				pins = "gpio117";
				drive-strength = <6>;
				bias-pull-up;
			};
		};
		tmd2725_sleep: tmd2725_sleep {
			config {
				pins = "gpio117";
				drive-strength = <2>;
				bias-pull-up;
			};
		};
	};
};
